{
  "functions": {
    "api/reverse-geocode.js": {
      "runtime": "nodejs18.x"
    },
    "api/forward-geocode.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/reverse-geocode",
      "destination": "/api/reverse-geocode.js"
    },
    {
      "source": "/api/forward-geocode",
      "destination": "/api/forward-geocode.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
